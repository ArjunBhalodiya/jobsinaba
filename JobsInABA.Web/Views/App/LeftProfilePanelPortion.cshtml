<div class="left-side">
    <div class="boxcompany profileimage" style="text-align: center; padding: 4px; border-top: 1px solid rgb(234, 234, 234); background-color: rgb(198, 200, 213);">
        <img id="profileimage" src="assets/img/job1.jpg" style="max-width:100%;">
        <input id="uploadimage" placeholder="Upload Image" style="display:none;" type="file">
        <img src="assets/img/edit-32.png" id="profile" onclick="updateprofile()" style="visibility: visible; width: 18px; cursor: pointer; position: relative;">
    </div>
    <div class="boxcompany">
        <h1 style="padding: 10px; text-align: center; color:#FE5500; padding-top: 25px;">Personal Detail</h1>
        <p id="name" class="editablelabel companydata even"><span class="text">First Name</span><span class="text"> Last Name</span></p>
        <p id="professionalstatus" class="editablelabel companydata odd"><span class="text">Professional Status</span></p>
        <p id="position" class="editablelabel companydata even"><span class="text">Current Position</span></p>
        <p id="companyname" class="editablelabel companydata odd"><span class="text">Company Name</span></p>
        <p id="service" class="editablelabel companydata even"><span class="text">Company's Services</span></p>
        <p id="dob" class="editablelabel companydata odd"><span class="text">BirthDate</span></p>
        <p id="maritalstatus" class="editablelabel companydata even"><span class="text">Marital Status</span></p>
        <h1 style="padding: 10px;
text-align: center;color:#FE5500;">
            Contact Detail
        </h1>
        <p id="address" class="editablelabel companydata even"><span class="text">City</span><span class="text">State</span>   <span class="text">Zip</span></p>
        <p id="email" class="editablelabel companydata odd"><span class="text">Email</span></p>
        <p id="phone" class="editablelabel companydata even"><span class="text">Phone</span><span class="text">   Cell</span></p>
    </div>
</div>
